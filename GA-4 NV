#include <iostream> // Подключаем библиотеку для работы с вводом-выводом
#include <fstream> // Подключаем библиотеку для работы с файлами
#include <vector> // Подключаем библиотеку для работы с динамическими массивами
#include <algorithm> // Подключаем библиотеку для использования алгоритмов сортировки

int main() {
    std::ifstream inputFile("input.txt"); // Открываем входной файл для чтения
    std::ofstream outputFile("output.txt"); // Открываем выходной файл для записи

    std::vector<int> numbers; // Создаем вектор для хранения чисел

    int num; // Переменная для временного хранения считанного числа
    while (inputFile >> num) { // Считываем числа из входного файла
        numbers.push_back(num); // Добавляем число в вектор
    }

    // Обрабатываем каждое число в соответствии с условием задачи
    for (int& n : numbers) {
        if (n > 0) {
            n += 1; // Если число больше нуля, увеличиваем его на 1
        } else if (n < 0) {
            n -= 1; // Если число меньше нуля, уменьшаем его на 1
        } else {
            numbers.clear(); // Если число равно нулю, очищаем вектор
            break; // Завершаем обработку
        }
    }

    // Сортируем числа в порядке возрастания
    std::sort(numbers.begin(), numbers.end());

    // Записываем результат в выходной файл
    for (int n : numbers) {
        outputFile << n << " "; // Записываем каждое число с пробелом
    }

    inputFile.close(); // Закрываем входной файл
    outputFile.close(); // Закрываем выходной файл

    return 0; // Завершаем программу
}
